{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": ["node_modules/(?!(llm-scraper)/)"],
  "setupFilesAfterEnv": ["<rootDir>/test/setup-e2e.ts"],
  "testTimeout": 30000
}
